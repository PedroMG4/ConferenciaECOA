{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.html)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/comprar",
      "dest": "/comprar.html"
    },
    {
      "src": "/gracias",
      "dest": "/gracias.html"
    },
    {
      "src": "/verificador",
      "dest": "/verificador.html"
    }
  ]
}
